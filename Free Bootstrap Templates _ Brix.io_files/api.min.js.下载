function Ajax(e,t,n,r,i,s){var o=new XMLHttpRequest;window.XMLHttpRequest?o=new XMLHttpRequest:o=new ActiveXObject("Microsoft.XMLHTTP");o.onreadystatechange=function(){if(o.readyState==4)if(o.status==200){var e=o.responseText;r(e)}else{o.abort();i!=null&&i()}};if(n=="GET"){o.open("GET",e+"?"+t,!0);o.send()}else{o.open("POST",e,!0);switch(s){case"file":break;default:o.setRequestHeader("Content-type","application/x-www-form-urlencoded")}o.send(t)}}function Api(e,t,n,r,i){var s,o="/api";switch(e){case"user":s=o+"/user.ne";break;case"sync":s=o+"/sync.ne";break;case"login":s=o+"/login.ne";break;case"logout":s=o+"/logout.ne";break;case"project":s=o+"/project.ne";break;case"pay":s=o+"/pay.ne";break;case"comment":s=o+"/comment.ne";break;case"log":s=o+"/log.ne";break;case"uploadfile":s=o+"/uploadfile.ne"}e=="uploadfile"?Ajax(s,t,"POST",n,r,i):Ajax(s,t,"GET",n,r,i)};